<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />
  <%= f.label "Artist" %>
  <%= f.text_field :artist_name %><br>

  <%= f.label "Genre" %>
  <%= f.collection_select :genre_name, Genre.all, :name, :name %><br>

  <%= f.label "Notes" %>

  <input name="song[note_contents][]">
  <input name="song[note_contents][]">
  <input name="song[note_contents][]"><br>

  <%= f.submit %>
<% end %>
